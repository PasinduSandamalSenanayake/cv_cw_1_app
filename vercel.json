{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://your-app.streamlit.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Forwarded-Host",
          "value": "$host"
        },
        {
          "key": "X-Forwarded-Proto",
          "value": "https"
        }
      ]
    }
  ]
}
